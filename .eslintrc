{
    "env": {
        "browser": true,
        "node": true,
        "es2017": true
    },
    "globals": {
        "$": false,
        "angular": false
    },
    "extends": ["prettier"],
    //"extends": ["prettier", "plugin:jsdoc/recommended"],
    //"plugins": ["jsdoc"],
    "rules": {
        "array-bracket-spacing": 2,
        "block-scoped-var": 0,
        "camelcase": [
            2,
            {
                "properties": "never"
            }
        ],
        "comma-style": [2, "last"],
        // "complexity": [2, 8],
        "curly": [2, "all"],
        "dot-notation": 0,
        "eol-last": 2,
        "eqeqeq": 2,
        // "func-names": 2,
        "guard-for-in": 2,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "keyword-spacing": 0,
        "linebreak-style": 0,
        "max-depth": [2, 5],
        "max-len": [2, 100],
        "max-params": [2, 40],
        "new-cap": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-cond-assign": 0,
        "no-debugger": 0,
        "no-empty": 2,
        "no-eq-null": 2,
        "no-eval": 0,
        "no-extend-native": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 0,
        "no-loop-func": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-str": 2,
        "no-multiple-empty-lines": 2,
        "no-new": 2,
        "no-new-func": 0,
        "no-new-wrappers": 0,
        "no-plusplus": 0,
        "no-proto": 0,
        "no-script-url": 0,
        "no-shadow": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-unused-expressions": 0,
        "no-unused-vars": 0,
        "no-use-before-define": [
            2,
            {
                "functions": false
            }
        ],
        "operator-linebreak": [2, "after"],
        "quotes": [2, "single"],
        "require-yield": 0,
        "semi": 0,
        "sort-imports": 2,
        "sort-keys": 1,
        "sort-vars": 2,
        "space-before-blocks": 0,
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": [
            2,
            {
                "nonwords": false,
                "overrides": {}
            }
        ],
        "strict": 0,
        "valid-jsdoc": 0,
        "valid-typeof": 0,
        "wrap-iife": [2, "any"]
    }
}
